<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<BorderPane prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <top>
       <MenuBar fx:id="fx_titleMenu" maxHeight="30.0" maxWidth="1.7976931348623157E308" prefHeight="30.0">
         <menus>
             <Menu mnemonicParsing="false" text="Settings">
                 <items>
                     <MenuItem mnemonicParsing="false" onAction="#openConnectionMenu" text="Connect" />
                     <MenuItem mnemonicParsing="false" onAction="#closeApplication" text="Close" />
                 </items>
             </Menu>
             <Menu mnemonicParsing="false" text="Strategies">
                 <items>
                     <MenuItem mnemonicParsing="false" text="Delete" />
                 </items>
             </Menu>
         </menus>
       </MenuBar>
   </top>
   <left>
      <TabPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="420.0" prefWidth="210.0" tabClosingPolicy="UNAVAILABLE" BorderPane.alignment="CENTER">
         <tabs>
            <Tab closable="false" text="Tickers">
               <content>
                  <ListView fx:id="fx_tickers" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" />
               </content>
            </Tab>
         </tabs>
      </TabPane>
   </left>
   <center>
      <VBox>
         <children>
            <TableView fx:id="fx_accounts" editable="true" maxWidth="1.7976931348623157E308" prefHeight="100.0">
              <columns>
                <TableColumn prefWidth="75.0" text="Account ID" />
                <TableColumn prefWidth="75.0" text="Broker" />
                  <TableColumn prefWidth="73.5999755859375" text="Currency" />
                  <TableColumn prefWidth="145.6000518798828" text="Total Cash" />
                  <TableColumn prefWidth="147.2000732421875" text="Invested Cash" />
                  <TableColumn prefWidth="104.00006103515625" text="Free Cash" />
              </columns>
            </TableView>
              <SplitPane dividerPositions="0.5" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" orientation="VERTICAL" prefHeight="590.0" prefWidth="1070.0">
                  <items>
                        <TabPane fx:id="fx_chartsTabPane" prefHeight="206.4" prefWidth="588.8" side="BOTTOM" tabClosingPolicy="ALL_TABS" />
                        <TabPane prefHeight="206.4" prefWidth="588.8" tabClosingPolicy="UNAVAILABLE">
                            <tabs>
                                <Tab text="Orders">
                                    <content>
                                        <TableView fx:id="fx_openOrders" prefHeight="200.0" prefWidth="200.0">
                                            <columns>
                                                <TableColumn prefWidth="75.0" text="Symbol" />
                                                <TableColumn prefWidth="75.0" text="Price" />
                                            </columns>
                                        </TableView>
                                    </content>
                                </Tab>
                                <Tab text="Closed Orders" />
                            </tabs>
                        </TabPane>
                  </items>
              </SplitPane>
         </children>
         <BorderPane.margin>
            <Insets />
         </BorderPane.margin>
      </VBox>
   </center>
</BorderPane>
