<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<BorderPane prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <top>
       <MenuBar fx:id="fx_titleMenu" maxHeight="30.0" maxWidth="1.7976931348623157E308" prefHeight="30.0">
         <menus>
             <Menu mnemonicParsing="false" text="Commands">
                 <items>
                     <MenuItem mnemonicParsing="false" onAction="#openConnectionMenu" text="Connect" />
                  <MenuItem mnemonicParsing="false" text="Demo App" />
                     <MenuItem mnemonicParsing="false" onAction="#closeApplication" text="Close" />
                 </items>
             </Menu>
         </menus>
       </MenuBar>
   </top>
   <left>
      <TabPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="420.0" prefWidth="210.0" tabClosingPolicy="UNAVAILABLE" BorderPane.alignment="CENTER">
         <tabs>
            <Tab closable="false" text="Tickers">
               <content>
                  <ListView fx:id="fx_tickers" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefWidth="80.0" />
               </content>
            </Tab>
         </tabs>
      </TabPane>
   </left>
   <center>
      <VBox>
         <children>
            <TableView fx:id="fx_accounts" maxWidth="1.7976931348623157E308" prefHeight="100.0">
              <columns>
                <TableColumn editable="false" maxWidth="-Infinity" minWidth="80.0" prefWidth="-1.0" text="Account ID" />
                <TableColumn maxWidth="-Infinity" minWidth="80.0" prefWidth="-1.0" text="Broker" />
                  <TableColumn maxWidth="-Infinity" minWidth="80.0" prefWidth="-1.0" text="Currency" />
                  <TableColumn maxWidth="-Infinity" minWidth="120.0" prefWidth="-1.0" text="Total Cash" />
                  <TableColumn maxWidth="-Infinity" minWidth="120.0" prefWidth="-1.0" text="Invested Cash" />
                  <TableColumn editable="false" maxWidth="-Infinity" minWidth="120.0" prefWidth="-1.0" sortable="false" text="Free Cash" />
              </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
              <SplitPane dividerPositions="0.5" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" orientation="VERTICAL" prefHeight="590.0" prefWidth="1070.0">
                  <items>
                        <TabPane fx:id="fx_chartsTabPane" prefHeight="206.4" prefWidth="588.8" side="BOTTOM" tabClosingPolicy="ALL_TABS" />
                        <TabPane prefHeight="206.4" prefWidth="588.8" tabClosingPolicy="UNAVAILABLE">
                            <tabs>
                        <Tab closable="false" text="Positions">
                           <content>
                              <TableView fx:id="fx_positions">
                                 <columns>
                                    <TableColumn prefWidth="75.0" text="Symbol" />
                                    <TableColumn prefWidth="75.0" text="Price" />
                                    <TableColumn prefWidth="75.0" text="Quantity" />
                                    <TableColumn prefWidth="92.80000305175781" text="Avg Entry" />
                                    <TableColumn prefWidth="72.800048828125" text="Profit Loss" />
                                 </columns>
                              </TableView>
                           </content>
                        </Tab>
                                <Tab closable="false" text="Orders">
                                    <content>
                                        <TableView fx:id="fx_orders" prefHeight="200.0" prefWidth="200.0">
                                            <columns>
                                                <TableColumn prefWidth="75.0" text="Symbol" />
                                                <TableColumn prefWidth="95.19995880126953" text="Creation Date" />
                                    <TableColumn prefWidth="76.00001525878906" text="Execution" />
                                    <TableColumn prefWidth="55.20001220703125" text="Side" />
                                    <TableColumn prefWidth="55.20001220703125" text="Quantity" />
                                    <TableColumn prefWidth="55.20001220703125" text="Status" />
                                    <TableColumn prefWidth="72.0" text="Avg Price" />
                                    <TableColumn prefWidth="70.39987182617188" text="Filled Date" />
                                            </columns>
                                        </TableView>
                                    </content>
                                </Tab>
                            </tabs>
                        </TabPane>
                  </items>
              </SplitPane>
         </children>
         <BorderPane.margin>
            <Insets />
         </BorderPane.margin>
      </VBox>
   </center>
</BorderPane>
